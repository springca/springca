<!-- GB/T 20518-2018 《信息安全技术 公钥基础设施 数字证书格式》 -->
<Certificate>
    <TBSCertificate>
        <!-- 版本号（本标准使用V3版本，值为2） -->
        <version>2</version>
        <!-- 证书序列号 -->
        <serialNumber/>
        <!-- 证书签名算法（应与signatureAlgorithm匹配） -->
        <signature choice="SHA1withRSA,SHA256withRSA,SM3withSM2"/>
        <!-- 颁发者（应与subject一致） -->
        <issuer>${subejct}</issuer>
        <!-- 有效期 -->
        <validity>
            <!-- 开始时间 -->
            <notBefore/>
            <!-- 终止时间 -->
            <notAfter/>
        </validity>
        <!-- 使用者 -->
        <subject>
            <!-- 国家 -->
            <country/>
            <!-- 省份 -->
            <province/>
            <!-- 地市 -->
            <city/>
            <!-- 组织名称 -->
            <organization/>
            <!-- 机构名称 -->
            <organizationUnit/>
            <!-- 服务器名称 -->
            <commonName/>
        </subject>
        <!-- 使用者公钥信息 -->
        <subjectPublicKeyInfo choice="RSA(2048+),SM2(256+)"/>
        <!-- 证书扩展项 TODO: 从此继续-->
        <extensions>
            <!-- 颁发者密钥标识符 -->
            <authorityKeyIdentifier critical="false"/>
            <!-- 使用者密钥标识符 -->
            <subjectKeyIdentifier critical="false"/>
            <!--&lt;!&ndash; 证书密钥用法（双证书标记为关键，单证书标记为非关键） &ndash;&gt;-->
            <!--<keyUsage critical="false"/>-->
            <!--&lt;!&ndash; 扩展密钥用途（如果密钥用法只限于所指的用途时标记为关键，否则标记为非关键） &ndash;&gt;-->
            <!--<extKeyUsage critical="false"/>-->
            <!--&lt;!&ndash; 使用者私钥使用期 &ndash;&gt;-->
            <!--<privateKeyUsagePeriod critical="false"/>-->
            <!--&lt;!&ndash; 证书策略 &ndash;&gt;-->
            <!--<certificatePolicies critical="false"/>-->
            <!--&lt;!&ndash; 策略映射（如果证书用户需要正确的解释CA的发布设定的规则时标识为关键，否则标识为非关键） &ndash;&gt;-->
            <!--<policyMappings critical="false"/>-->
            <!--&lt;!&ndash; 使用者别名 &ndash;&gt;-->
            <!--<subjectAlgName critical="false"/>-->
            <!--&lt;!&ndash; 颁发者别名 &ndash;&gt;-->
            <!--<issuerAltName critical="false"/>-->
            <!--&lt;!&ndash; 主体目录属性 &ndash;&gt;-->
            <!--<subjectDirectoryAttributes critical="false"/>-->
            <!--&lt;!&ndash; 证书基本限制（CA证书标记为关键，终端实体证书标记为非关键） &ndash;&gt;-->
            <!--<basicConstraints critical="false"/>-->
            <!--&lt;!&ndash; 名称限制（如果证书用户系统应检验所处理的认证路径与此扩展中的值是否一致时标记为关键，否则标记为非关键） &ndash;&gt;-->
            <!--<nameConstraints critical="false"/>-->
            <!--&lt;!&ndash; 策略限制（如果证书用户需要正确的解释认证机构CA设定的规则时标识为关键，否则标识为非关键） &ndash;&gt;-->
            <!--<policyConstraints critical="false"/>-->
            <!-- CRL分发点 -->
            <cRLDistributionPoints critical="false"/>
            <!--&lt;!&ndash; 限制所有策略（如果证书用户需要正确的解释认证机构CA设定的规则时标识为关键，否则标识为非关键） &ndash;&gt;-->
            <!--<inhibitAnyPolicy critical="false"/>-->
            <!--&lt;!&ndash; 最新的CRL &ndash;&gt;-->
            <!--<freshestCRL critical="false"/>-->
            <!--&lt;!&ndash; 私有的Internet扩展 &ndash;&gt;-->
            <!--<id_pkix critical="false"/>-->
            <!--&lt;!&ndash; 颁发机构信息访问 &ndash;&gt;-->
            <!--<authorityInfoAccess critical="false"/>-->
            <!--&lt;!&ndash; 主体信息访问 &ndash;&gt;-->
            <!--<subjectInformationAccess critical="false"/>-->
            <!--&lt;!&ndash; 个人身份证号码 &ndash;&gt;-->
            <!--<identifyCardNumber critical="false"/>-->
            <!--&lt;!&ndash; 个人社会保险号 &ndash;&gt;-->
            <!--<inuranceNumber critical="false"/>-->
            <!--&lt;!&ndash; 企业工商注册号 &ndash;&gt;-->
            <!--<iCRegistrationNumber critical="false"/>-->
            <!--&lt;!&ndash; 企业组织机构代码 &ndash;&gt;-->
            <!--<organizationCode critical="false"/>-->
            <!--&lt;!&ndash; 企业税号 &ndash;&gt;-->
            <!--<taxationNumber critical="false"/>-->
        </extensions>
    </TBSCertificate>
    <!-- 颁发签名算法 -->
    <signatureAlgorithm/>
    <!-- 颁发者签名 -->
    <signatureValue/>
</Certificate>
